{
  "name": "@repo/launchpad-contract",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "yarn@4.9.2",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "compact": "run-compactc src/launchpad.compact ./src/managed/launchpad",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -R ./src/managed ./dist/managed && cp ./src/launchpad.compact dist",
    "build:start": "run compact && run build",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@midnight-ntwrk/compact": "*",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@noble/hashes": "^2.0.0"
  }
}
